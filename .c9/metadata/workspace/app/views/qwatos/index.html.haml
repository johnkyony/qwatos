{"changed":true,"filter":false,"title":"index.html.haml","tooltip":"/app/views/qwatos/index.html.haml","value":"\n.row\n  .col-sm-6.col-md-4.col-md-offset-4\n    - @qwatos.each do |qwato|\n      .thumbnail{:id => \"qwato_#{qwato.id}\"}\n        = image_tag qwato.image_url , alt: qwato.quote\n        %br/\n        - if user_signed_in?\n          / Twitter\n          %a.btn.btn-primary{:href => \"http://twitter.com/home?status=#{qwato.image_url}\", :target => \"_blank\", :title => \"Share on Twitter\"}\n            %i.fa.fa-twitter\n            Twitter\n          / Facebook\n          %a.btn.btn-primary{:href => \"https://www.facebook.com/sharer/sharer.php?u=#{qwato.image_url}\", :target => \"_blank\", :title => \"Share on Facebook\"}\n            %i.fa.fa-facebook\n            Facebook\n          = link_to(qwato_likes_path(qwato), |\n            method: :post ) do              |\n            = content_tag('i', nil, class: 'fa fa-thumbs-up fa-2x pull-right' )\n          = link_to(qwato_dislikes_path(qwato), |\n            method: :post ) do                 |\n            = content_tag('i', nil, class: 'fa fa-thumbs-down fa-2x pull-right' )\n        - else\n          = button_to \"share\" , user_registration_path , class: \"btn btn-primary pull-left\"\n          = button_to \"like\", user_registration_path, class: \"btn btn-success \"\n      %br/\n\n\n","undoManager":{"mark":-1,"position":-1,"stack":[]},"ace":{"folds":[],"scrolltop":120,"scrollleft":0,"selection":{"start":{"row":26,"column":0},"end":{"row":26,"column":0},"isBackwards":false},"options":{"guessTabSize":true,"useWrapMode":false,"wrapToView":true},"firstLineState":{"row":7,"state":"embedded_ruby","mode":"ace/mode/haml"}},"timestamp":1432050784000}